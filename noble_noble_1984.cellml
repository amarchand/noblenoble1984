    <dc:title>
            A Model of Sino-Atrial Node Electrical Activity Based on a                       Modification of the DiFrancesco-Noble (1984) Equations
          </dc:title>
    <bqs:volume>222</bqs:volume>
    <bqs:first_page>295</bqs:first_page>
